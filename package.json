{
  "name": "incode",
  "version": "1.0.0",
  "description": "Incode allows users to view their software project in terms of a software city. The initial version of incode was made for a semester project at the OST.",
  "main": "index.ts",
  "scripts": {
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "start": "npx webpack serve --config webpack.dev.js",
    "build": "npx webpack --config webpack.prod.js",
    "lint": "npx eslint . --ext .ts,.tsx",
    "lint:fix": "npx eslint . --ext .ts,.tsx --fix",
    "circular-dependencies": "node node_modules/madge/bin/cli.js --extensions ts --circular ./src --exclude 'internal.ts|internal.lazy.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.ost.ch/sa_webvr/incode.git"
  },
  "author": "Thomas Hindermann & Joel Hirzel",
  "license": "ISC",
  "devDependencies": {
    "@babylonjs/core": "^4.2.0",
    "@babylonjs/loaders": "^4.2.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babylonjs": "^4.2.0",
    "babylonjs-loaders": "^4.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.4.0",
    "java-parser": "^2.0.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@babylonjs/gui": "^4.2.0",
    "@babylonjs/materials": "^4.2.0",
    "jest": "^27.3.1",
    "madge": "^5.0.1",
    "webpack-merge": "^5.8.0"
  }
}
